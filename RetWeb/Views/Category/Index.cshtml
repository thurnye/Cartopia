@model List<Category>

<h1>category list</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Category Name</th>
            <th scope="col">Display Order</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(u => u.DisplayOrder)){
            <tr>
                <th scope="row">@obj.Id</th>
                <td>@obj.Name</td>
                <td>@obj.DisplayOrder</td>
            </tr>
        }
        
    </tbody>
</table>